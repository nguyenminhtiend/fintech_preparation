# Database
DATABASE_URL="postgresql://user:password@localhost:5432/fintech_db"

# Server
NODE_ENV="development"
PORT=3000
LOG_LEVEL="info"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
